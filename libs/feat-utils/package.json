{
    "name": "@galaxar/feat-utils",
    "version": "1.0.1",
    "description": "Node common application utility features",
    "main": "cjs/index.js",
    "module": "lib/index.js",
    "react-native": "lib/index.js",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./cjs/index.js"
        },
        "./*": {
            "import": "./lib/*.js",
            "require": "./cjs/*.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "pnpm build && mocha --recursive test/**/*.spec.js",
        "cover": "nyc --reporter=html --reporter=text mocha --recursive test/**/*.spec.js && open ./coverage/index.html",
        "prettier": "prettier --write .",
        "lint": "eslint \"**/*.js\"",
        "lint-fix": "eslint \"**/*.js\" --fix",
        "clean": "shx rm -rf cjs",
        "build": "pnpm clean && swc lib/**/*.js -d ./cjs"
    },
    "keywords": [
        "galaxar",
        "app",
        "common",
        "feature"
    ],
    "nyc": {
        "exclude": [
            ".*",
            "cjs",
            "test",
            "**/*.spec.js"
        ]
    },
    "dependencies": {
        "@galaxar/app": "workspace:*",
        "@galaxar/utils": "workspace:*",
        "hasha": "^5.2.2",
        "node-cache": "^5.1.2",
        "sharp": "^0.32.1"
    },
    "prettier": {
        "quoteProps": "consistent",
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "useTabs": false
    },
    "license": "MIT",
    "devDependencies": {
        "capture-console": "^1.0.1"
    }
}
