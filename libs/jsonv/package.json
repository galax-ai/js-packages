{
    "name": "@galaxar/jsonv",
    "version": "1.0.0",
    "description": "JSON Validation Syntax",
    "main": "cjs/index.js",
    "module": "lib/index.js",
    "react-native": "lib/index.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "mocha --recursive test/**/*.spec.js",
        "cover": "nyc --reporter=html --reporter=text mocha --recursive test/**/*.spec.js && open ./coverage/index.html",
        "prettier": "prettier --write .",
        "lint": "eslint \"**/*.js\"",
        "lint-fix": "eslint \"**/*.js\" --fix",
        "clean": "shx rm -rf cjs",
        "build": "pnpm clean && swc lib/*.js -d ./cjs"
    },
    "keywords": [
        "galaxar",
        "json",
        "validation",
        "validator"
    ],
    "nyc": {
        "exclude": [
            ".*",
            "cjs",
            "test",
            "**/*.spec.js"
        ]
    },
    "dependencies": {
        "@galaxar/utils": "workspace:*",
        "@galaxar/types": "workspace:*"
    },
    "devDependencies": {},
    "license": "MIT"
}