{
    "name": "@galaxar/types",
    "version": "1.0.5",
    "description": "Galaxar Semantic Data Types",
    "main": "cjs/index.js",
    "module": "lib/index.js",
    "react-native": "lib/index.js",
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "require": "./cjs/index.js"
        },
        "./*": {
            "import": "./lib/*.js",
            "require": "./cjs/*.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "pnpm build && mocha --recursive ./test/*.spec.js",
        "cover": "nyc --reporter=html --reporter=text mocha --recursive ./test/*.spec.js && open ./coverage/index.html",
        "prettier": "prettier --write .",
        "lint": "eslint \"**/*.js\"",
        "lint-fix": "eslint \"**/*.js\" --fix",
        "clean": "shx rm -rf cjs",
        "build": "pnpm clean && swc ./lib/**/*.js -d ./cjs"
    },
    "devDependencies": {
        "luxon": "^3.3.0"
    },
    "dependencies": {
        "@galaxar/utils": "workspace:*",
        "lodash": "^4.17.21"
    },
    "nyc": {
        "exclude": [
            ".*",
            "cjs",
            "test"
        ]
    },
    "keywords": [
        "galaxar",
        "types"
    ],
    "prettier": {
        "quoteProps": "consistent",
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "useTabs": false
    },
    "license": "MIT"
}
